{
  "name": "alti",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev:server": "nodemon ./server/bin/www | bunyan -L -o short",
    "dev:client": "webpack --watch --config webpack.dev.js",
    "lint": "precise-commits",
    "start": "node ./server/bin/www",
    "start-dev": "NODE_ENV=development run-p dev:*",
    "test": "NODE_ENV=test jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@instructure/ui-elements": "^5.34.0",
    "@instructure/ui-layout": "^5.34.0",
    "@instructure/ui-presets": "^5.34.0",
    "@instructure/ui-themes": "^5.34.0",
    "@sentry/node": "^4.3.4",
    "babel-loader": "^8.0.2",
    "bunyan": "^1.8.12",
    "buzzapi": "^1.2.4",
    "clean-webpack-plugin": "^1.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~4.1.0",
    "dotenv": "^6.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "html-webpack-plugin": "^3.2.0",
    "http-errors": "~1.7.2",
    "ims-lti": "^3.0.2",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "parse-link-header": "^1.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-lti": "^0.0.7",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-ga": "^2.5.5",
    "superagent": "^4.0.0",
    "superagent-promise": "^1.1.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0"
  }
}
